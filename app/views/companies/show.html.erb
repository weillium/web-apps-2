<h1><%= @company.name %></h1>
<h2><%= @company.city %>, <%= @company.state %></h2>

<ul>
    <% for contact in @company.contacts %>
        <li><%= contact.last_name %>, <%= contact.first_name %></li>
    <% end %>
    <li><a href="/contacts/new?company_id=<%= @company.id %>">Create new contact</a></li>
</ul>

<a href="/companies">Go back to all companies</a>